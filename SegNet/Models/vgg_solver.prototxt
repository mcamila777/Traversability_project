net: "SegNet/Models/vgg_train_val.prototxt"  	# Change this to the absolute path to your model file
test_initialization: false
test_iter: 1
test_interval: 10000000
base_lr: 0.001
lr_policy: "step"
gamma: 1.0
stepsize: 100000
display: 100#400
momentum: 0.9
max_iter: 350000
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "SegNet/Models/Training/vgg16_"  	# Change this to the absolute path to where you wish to output solver snapshots
solver_mode: GPU
